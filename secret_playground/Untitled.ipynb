    "import numpy as np\n",
    "\n",
    "x = 0.9\n",
    "T = 10\n",
    "R1 = 1\n",
    "R0 = 100\n",
    "\n",
    "for t in range(T):\n",
    "    u = np.random.binomial(1,x)\n",
    "    print(\"u=%d\" % u)\n",
    "    if u==1:\n",
    "        G = R1/x\n",
    "    else:\n",
    "        G = -R0/(1-x)\n",
    "    x += .01*G\n",
    "    x = min(max(x,0),1)\n",
    "    print(x)"
   ]
